          <argument>
            <kind>1</kind>
            <string>// Menu processing.

// Checks to see if the confirm key is pressed.
if ( SCR_KEY_CHECK( CONFIRM_KEY ) )
{
    // Checks to see which option the player has chosen.
    // -    First option starts the game.
    //      Second option quits the game.
    if (global.menu_current_option == 0)
    {
        // Attempts to load the next map.
        // -    Uses macro TITLE_SCREEN_ID to decide the map.
        //      Note: In SCR_MAP_DONE a hack can redirect the map to a
        //          defined map, for testing. Turning off DEV_TOOLS will
        //          make sure the program works as intended.
        if !SCR_MAP_DONE(TITLE_SCREEN_ID) {show_message("Next Map Broken");}
        
    }
    else
    {
        // Quits the game.
        game_end();
    }
}
</string>
