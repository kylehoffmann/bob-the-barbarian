          <argument>
            <kind>1</kind>
            <string>// Draws Menu text.

// Set colour for menu items not currently selected.
draw_set_color(c_gray);
// Sets the menu colour to black if the current option is selected.
// -    In this case the user looking at option 1 (index 0).
if (global.menu_current_option == 0) draw_set_color(c_black);
// User is looking to begin the game, the message prints "Start".
// -    As the first selection it is printed at the start x &amp; y.
draw_text_transformed(global.menu_options_start_x, global.menu_options_start_y, "Start", global.menu_options_scale, global.menu_options_scale, 0);
// Set colour for menu items not currently selected.
draw_set_color(c_gray);
// Sets the menu colour to black if the current option is selected.
// -    In this case the user looking at option 2 (index 1).
if (global.menu_current_option == 1) draw_set_color(c_black);
// User is looking to End the game, the message prints "Quit".
// -    As the second selection it is printed at the start x and
//          the start y + a spacing defined by global.menu_options_spacing.
draw_text_transformed(global.menu_options_start_x, global.menu_options_start_y + global.menu_options_spacing, "Quit", global.menu_options_scale, global.menu_options_scale, 0);
</string>
