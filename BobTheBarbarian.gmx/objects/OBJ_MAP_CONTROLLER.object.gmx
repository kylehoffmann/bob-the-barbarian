            <kind>1</kind>
            <string>// Draws menu text.

// Checks if menu is open.
if (global.menu_open)
{
    // Finds the x&amp;y values to print the text.
    text_start_x = global.menu_options_start_x + view_xview[0];
    text_start_y = global.menu_options_start_y + view_yview[0];
    
    // Set colour for menu background.
    // -    TO_DO: Add a border.
    draw_set_color(c_white);
    // Draw background at start points defined above.
    // -    TO_DO: Find a way to set the menu more dynamically
    draw_rectangle(text_start_x, text_start_y, text_start_x + 300, text_start_y+ 170, false);
    // Set colour for menu items not currently selected.
    draw_set_color(c_gray);
    // Sets the menu colour to black if the current option is selected.
    // -    In this case the user looking at option 1 (index 0).
    if (global.menu_current_option == 0) draw_set_color(c_black);
    // User is looking to Reset the current map, the message prints "Reset Map".
    // -    As the first selection it is printed at the start x &amp; y.
    draw_text_transformed(text_start_x, text_start_y, "Reset Map", global.menu_options_scale, global.menu_options_scale, 0);
    // Set colour for menu items not currently selected.
    draw_set_color(c_gray);
    // Sets the menu colour to black if the current option is selected.
    // -    In this case the user looking at option 2 (index 1).
    if (global.menu_current_option == 1) draw_set_color(c_black);
    // User is looking to Reset the game, the message prints "Reset Game".
    // -    As the second selection it is printed at the start x and
    //          the start y + a spacing defined by global.menu_options_spacing.
    draw_text_transformed(text_start_x, text_start_y + global.menu_options_spacing, "Reset Game", global.menu_options_scale, global.menu_options_scale, 0);
    // Set colour for menu items not currently selected.
    draw_set_color(c_gray);
    // Sets the menu colour to black if the current option is selected.
    // -    In this case the user looking at option 3 (index 2).
    if (global.menu_current_option == 2) draw_set_color(c_black);
    // User is looking to  Quit the game, the message prints "Quit Game".
    // -    As the third selection it is printed at the start x and
    //          the start y + 2 * the spacing defined by 
    //          global.menu_options_spacing.
    draw_text_transformed(text_start_x, text_start_y + 2 * global.menu_options_spacing, "Quit", global.menu_options_scale, global.menu_options_scale, 0);
}

</string>
