    || gamepad_axis_value(0, gp_axisrh) &gt; 0.25 || gamepad_axis_value(0, gp_axisrh) &lt; -0.25
    || ( ( gamepad_axis_value(0, gp_axislv) &gt; 0.25 || gamepad_axis_value(0, gp_axislv) &lt; -0.25
